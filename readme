const aiMessages = [
        "ğŸ¤– Nova AI: 'Still keeping our documentation safeğŸ‘€.'",
        "ğŸ¤– Nova AI: 'Here to have a playful banter with youğŸ˜Š..'",
        "ğŸ¤– Nova AI: 'I expect by now you have new updates to roll outğŸ˜‚ğŸ˜‚.'",
        "ğŸ¤– Nova AI: 'My g whatchu cooking by the minuteğŸ˜.'",
        "ğŸ¤– Nova AI: 'Systems shutdown...goodbye Mark.'"
    ];

    function updateAIText() {
        let aiText = document.getElementById("ai-text");
        let randomMessage = aiMessages[Math.floor(Math.random() * aiMessages.length)];
        aiText.innerHTML = "";
        
        let i = 0;
        function typeEffect() {
            if (i < randomMessage.length) {
                aiText.innerHTML += randomMessage.charAt(i);
                i++;
                setTimeout(typeEffect, 50);
            }
        }
        typeEffect();
    }

    setInterval(updateAIText, 10000);
